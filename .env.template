# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Logging Level (Default: INFO)
LOG_LEVEL=INFO

# Maximum number of retries for API requests (Default: 3)
MAX_RETRIES=3

# Timeout for API requests in seconds (Default: 30)
API_TIMEOUT=30

# Base URL for the API (Default: https://api.openai.com)
API_BASE_URL=https://api.openai.com

# Other environment variables can be added here with their expected and default values
